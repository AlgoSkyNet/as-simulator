<?xml version="1.0" encoding="UTF-8"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xsi:noNamespaceSchemaLocation="http://as.tibco.com/simulator.xsd">

  <space name="customer" size="1000">
    <sequence field="id" key="true" />
    <firstName field="firstName" />
    <lastName field="lastName" />
    <address field="address" />
    <birthDate field="dob" />
    <regex field="phone" regex="[2-9][0-9]{2}-[0-9]{3}-[0-9]{4}" />
    <emailAddress field="email" />
    <words field="notes" />
    <city field="city" />
    <now field="creationTime" />
  </space>

  <space name="quote" sleep="1000">
    <item field="ticker" key="true">
      <value xsi:type="xsd:string">TIBX</value>
      <value xsi:type="xsd:string">GOOG</value>
      <value xsi:type="xsd:string">WFM</value>
      <value xsi:type="xsd:string">INTC</value>
      <value xsi:type="xsd:string">AMZN</value>
      <value xsi:type="xsd:string">AAPL</value>
      <value xsi:type="xsd:string">COST</value>
      <value xsi:type="xsd:string">DELL</value>
      <value xsi:type="xsd:string">EXPE</value>
      <value xsi:type="xsd:string">EBAY</value>
      <value xsi:type="xsd:string">MYL</value>
      <value xsi:type="xsd:string">VIAB</value>
      <value xsi:type="xsd:string">FB</value>
    </item>
    <boolean field="owned" />
    <now field="time" />
    <double field="bid" min="0" max="1000" decimals="2" />
    <double field="ask" min="0" max="1000" decimals="2" />
    <integer field="bidSize" min="0" max="1000" />
    <integer field="askSize" min="0" max="1000" />
    <double field="lastSale" min="0" max="1000" decimals="2" />
    <integer field="lastSize" min="0" max="1000" />
    <dateTime field="tradeTime" start="2013-10-15T07:20:00-05:00"
      end="2013-10-15T17:20:00-05:00" />
    <item field="exchange">
      <value xsi:type="xsd:string">NYSE</value>
      <value xsi:type="xsd:string">NASDAQ</value>
      <value xsi:type="xsd:string">AMEX</value>
    </item>
    <integer field="volume" min="0" max="10000" />
  </space>

  <space name="position" size="100000">
    <integer field="portfolioId" key="true" min="1" max="100000" />
    <item field="ticker" key="true">
      <value xsi:type="xsd:string">TIBX</value>
      <value xsi:type="xsd:string">GOOG</value>
      <value xsi:type="xsd:string">WFM</value>
      <value xsi:type="xsd:string">INTC</value>
      <value xsi:type="xsd:string">AMZN</value>
      <value xsi:type="xsd:string">AAPL</value>
      <value xsi:type="xsd:string">COST</value>
      <value xsi:type="xsd:string">DELL</value>
      <value xsi:type="xsd:string">EXPE</value>
      <value xsi:type="xsd:string">EBAY</value>
      <value xsi:type="xsd:string">MYL</value>
      <value xsi:type="xsd:string">VIAB</value>
      <value xsi:type="xsd:string">FB</value>
    </item>
    <integer field="customerId" min="0" max="1000" />
    <integer field="quantity" max="1000" />
  </space>

</simulation>
