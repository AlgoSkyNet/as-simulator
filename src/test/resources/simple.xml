<?xml version="1.0" encoding="UTF-8"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xsi:noNamespaceSchemaLocation="http://as.tibco.com/simulator.xsd">
  <space name="people" size="100000">
    <sequence fieldName="id" />
    <firstName />
    <lastName />
    <birthDate fieldName="dob" />
    <address fieldName="address" />
    <city fieldName="city" />
    <regex fieldName="phone" regex="[2-9][0-9]{2}-[0-9]{3}-[0-9]{4}" />
    <emailAddress fieldName="email" />
  </space>
</simulation>